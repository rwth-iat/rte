			while( (IteratorObj = IteratorObj.parentNode) && IteratorObj !== null && IteratorObj.namespaceURI == HMI.HMI_Constants.NAMESPACE_SVG){
				if(IteratorObj.getAttribute(\"display\") === \"none\"){
