	this.KSCONSTANTS.KS_VT_UINT           = \"0x00000011\";
	this.KSCONSTANTS.KS_VT_SINGLE         = \"0x00000020\";
	this.KSCONSTANTS.KS_VT_DOUBLE         = \"0x00000021\";
	this.KSCONSTANTS.KS_VT_STRING         = \"0x00000030\";
	this.KSCONSTANTS.KS_VT_TIME           = \"0x00000031\";
