		}else{
			clickPosition[0] = mousePosX;
			clickPosition[1] = mousePosY;
			
			this.hmi_log_trace(\"HMI.prototype.getClickPosition absolute - End x:\"+clickPosition[0]+\" y:\"+clickPosition[1]);
		}
		mousePosX = null;
		mousePosY = null;
		
		return clickPosition;
