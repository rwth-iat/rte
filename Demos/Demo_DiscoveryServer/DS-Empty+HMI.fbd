			clickPosition[0] = mousePosX - parseInt(Component.getAttribute(\"absolutex\"), 10) - svgOffsetX;
			clickPosition[1] = mousePosY - parseInt(Component.getAttribute(\"absolutey\"), 10) - svgOffsetY;
			
			svgOffsetX = null;
			svgOffsetY = null;
			
