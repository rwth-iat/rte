	//[StyleDescription] adjust this if no ACPLT/HMI Server has a StyleDescription anymore
	parse: function(GraphicDescription, StyleDescription) {
		HMI.hmi_log_trace(\"HMIDOMParser.parse - Start\");
		var GraphicElement;
		var StyleElement;
