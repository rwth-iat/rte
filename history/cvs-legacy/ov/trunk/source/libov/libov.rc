   VALUE "Author", "Dirk Meyer <dirk@plt.rwth-aachen.de>\000"
   VALUE "License", OV_VER_LICENSE "\000"
  }

 }

 BLOCK "VarFileInfo"
 {
  VALUE "Translation", 0x409, 1252
 }

}

OV_ICON ICON "libov.ico"

LANGUAGE 0x9,0x1
OV_MESSAGE MESSAGE "libov.bin"

