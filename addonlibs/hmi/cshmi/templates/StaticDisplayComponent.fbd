				var svgContent =	\"<svg:svg xmlns:svg='\"+HMI.HMI_Constants.NAMESPACE_SVG+\"' xmlns='\"+HMI.HMI_Constants.NAMESPACE_SVG+\"' \"
				+\"xmlns:xlink='\"+HMI.HMI_Constants.NAMESPACE_XLINK+\"'>\"
				+HTML
				+\"</svg:svg>\";
				HTMLcontentNode = HMI.HMIDOMParser.parse(svgContent);
				
